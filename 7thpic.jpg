    <script>
    $(document).ready(function() {
      var sidebarTestimonialIndex = 0;
      var sidebarTestimonialCount = 7;
      var changingTestimonial = false;

      function updateSidebarTestimonialIndex() {
        $('.sidebar-testimonials-nav .index').removeClass('fa-circle').addClass('fa-circle-thin');
        $('.sidebar-testimonials-nav .index[data-testimonial-index=' + sidebarTestimonialIndex + ']').removeClass('fa-circle-thin').addClass('fa-circle');
        $('.sidebar-testimonials-carousel').carousel(sidebarTestimonialIndex);
      }

      $('.sidebar-testimonials-carousel').on('slide.bs.carousel', function (e) {
        changingTestimonial = true;
        sidebarTestimonialIndex = e.to;
        $('.sidebar-testimonials-nav .index').removeClass('fa-circle').addClass('fa-circle-thin');
        $('.sidebar-testimonials-nav .index[data-testimonial-index=' + sidebarTestimonialIndex + ']').removeClass('fa-circle-thin').addClass('fa-circle');
      });

      $('.sidebar-testimonials-carousel').on('slid.bs.carousel', function (e) {
        changingTestimonial = false;
      });

      $('.sidebar-testimonials-carousel').carousel({interval: 5000, ride: 'carousel'});

      $('.sidebar-testimonials .previous').click(function() {
        if (changingTestimonial) {
          return;
        }
        sidebarTestimonialIndex--;
        if (sidebarTestimonialIndex < 0) {
          sidebarTestimonialIndex = sidebarTestimonialCount - 1;
        }
        updateSidebarTestimonialIndex();
      });

      $('.sidebar-testimonials .next').click(function() {
        if (changingTestimonial) {
          return;
        }
        sidebarTestimonialIndex++;
        if (sidebarTestimonialIndex >= sidebarTestimonialCount) {
          sidebarTestimonialIndex = 0;
        }
        updateSidebarTestimonialIndex();
      });

      $('.sidebar-testimonials .index').click(function() {
        sidebarTestimonialIndex = $(this).data('testimonial-index');
        updateSidebarTestimonialIndex();
      });
    });
    </script>

